/* 
    Processing.js Ref Test
    Function : append()
    Source   : http://processing.org/reference/append_
*/

 	
String[] sa1 = { "OH ", "NY ", "CA "}; 
String[] sa2 = append(sa1, "MA "); 


//** REFTEST VALUES **//
background(
  reftest.checkValues([
     sa2[0] === "OH ",
     sa2[1] === "NY ",
     sa2[2] === "CA ",
     sa2[3] === "MA "
     // need checks for other 'types'
  ])
);
exit();

